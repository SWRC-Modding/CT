--recurse=yes
--exclude=.git
--exclude=.vscode
--exclude=ClassInfo
--exclude=.\*\Debug
--exclude=.\*\Release
--exclude=.\Core\Core.c
--exclude=.\CTGame\CTGame.c
--exclude=.\CTMarkers\CTMarkers.c
--exclude=.\D3DDrv\D3DDrv.c
--exclude=.\Editor\Editor.c
--exclude=.\Engine\Engine.c
--exclude=.\Gameplay\Gameplay.c
--exclude=.\GameSpyMgr\GameSpyMgr.c
--exclude=.\IpDrv\IpDrv.c
--exclude=.\OpenGLDrv\Src\GL\glew.c
--exclude=.\Window\Window.c
--exclude=.\XGame\XGame.c
--exclude=.\XInterface\XInterface.c
--c++-kinds=+pf
--langdef=unrealscript
--langmap=unrealscript:.uc
--regex-unrealscript=/^[ \t]*[cC]lass[ \t]*([a-zA-Z0-9_]+)/\1/c,class/
--regex-unrealscript=/^[ \t]*[sS]tate[() \t]*([a-zA-Z0-9]+)/\1/s,state/
--regex-unrealscript=/^[ \t]*[sS]imulated[ \t]*(.*)[ \t]+([a-zA-Z0-9_]+)\(/\2/i,simulated/
--regex-unrealscript=/^[ \t]*[eE]vent[ \t]*[^ \t]*[ \t]+([a-zA-Z0-9_]+)\(/\1/e,event/
--regex-unrealscript=/^[ \t]*[fF]unction[ \t]*(.*)[ \t]+([a-zA-Z0-9_]+)\(/\2/f,function/
