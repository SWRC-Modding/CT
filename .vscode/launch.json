	"version": "0.2.0",
	"configurations": [
		{
			"preLaunchTask": "build",
			"name": "debug",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/../GameData/System/CT.exe",
			"args": [
				"testmap?game=MPGame.DMGame?goalscore=5?respawndelay=0?maxplayers=32?Listen",
				"-windowed",
				"-log",
				"-nosaveconfig",
				"-rendev=opengl"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"console": "internalConsole"
		}
	]
}
